	//中间弹窗
	function $openM(){
		setTimeout("switch_flag()",100000);
	}
	//中间弹窗显示，右下角弹窗关闭
	function switch_flag(){
		$("#divM").fadeIn(0);
		$(".swtBottom").animate({width:0,height:0},500);
	}
	var t;
	//关闭中间弹窗
	function closeM(){
		$("#divM").addClass("off");
		//打开右下角
		setTimeout(function(){
			$(".swtBottom").animate({width:250+"px",height:215+'px'},6000)
		},800);
		t=setTimeout(function(){
			$("#divM").removeClass("off");
			$(".swtBottom").animate({width:0,height:0},6000);
		},19000)
	}
	
	//输入内容直接跳至商务通
	$(function(){
		$openM();
		$("#divM .send_info_left textarea , .swtBottom #myInput").click(function(e) {
            openZoosUrl();
        });	
		
		//关闭右下角
		$(".swtBottom").find(".closeBtn").on("click",function(){
			$(".swtBottom").animate({width:0,height:0},800,function(){
				clearTimeout(t);
				setTimeout(function(){
					$("#divM").removeClass("off");
				},19000)
				
			})
		});
			//enter键发送
		$(".swtBottom").find("textarea").keyup(function (e) {  
		   if (e.which == 13){  
				$(".chatTxt>a").click();
		   }  
		});
	
	});